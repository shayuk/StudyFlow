{
  "functions": {
    "api/index.ts": { "runtime": "@vercel/node@3" },
    "api/health.ts": { "runtime": "@vercel/node@3" }
  },
  "routes": [
    { "src": "/api/health", "dest": "/api/health.ts" },
    { "src": "/api/(.*)", "dest": "/api/index.ts" },
    { "src": "/docs/(.*)", "dest": "/api/index.ts" }
  ]
}
